        android:pivotX="54"
        android:pivotY="54"
        android:scaleX="1.15"
        android:scaleY="1.15">

        <!-- PASTE all original <path> / <group> content from your
             adaptive icon foreground vector here, unchanged. -->
        <!-- Example:
        <path
            android:fillColor="#134AC1"
            android:pathData="M ..."/>
        -->
    </group>
</vector>
