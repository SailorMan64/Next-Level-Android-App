<?xml version="1.0" encoding="utf-8"?>
<!-- Copy of your launcher foreground vector, with a scale group added -->
<vector xmlns:android="http://schemas.android.com/apk/res/android"
    android:width="24dp"
    android:height="24dp"
    android:viewportWidth="108"
    android:viewportHeight="108">

    <!-- Zoom the content ~12â€“18%. Pivot at center of viewport (54,54). -->
